library(ggplot2)
library(ggforce)
n <- 12
theta <- seq(0, 2*pi, length.out = n + 1)[-(n + 1)]
petals <- data.frame(
x0 = 0.55 * cos(theta),
y0 = 0.55 * sin(theta),
a  = 0.28,   # petal length (semi-major axis)
b  = 0.11,   # petal width  (semi-minor axis)
angle = theta
)
p <- ggplot() +
ggforce::geom_ellipse(
data = petals,
aes(x0 = x0, y0 = y0, a = a, b = b, angle = angle),
fill = "white", color = "#E6C200", linewidth = 2
) +
geom_point(aes(0, 0), size = 18, color = "#F2C94C") +
coord_equal(xlim = c(-1, 1), ylim = c(-1, 1), expand = FALSE) +
theme_void()
# Save a square icon with transparent background
ggsave("daisy_icon_256.png", p, width = 1, height = 1, units = "in",
dpi = 256, bg = "transparent")
